<%= form_for(@doctor) do |f| %>
  <% if @doctor.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@doctor.errors.count, "error") %> prohibited this doctor from being saved:</h2>

      <ul>
      <% @doctor.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
<form role="form">
  <div class="form-group">
    <%= f.label :Nombre %><br>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :Direccion %><br>
    <%= f.text_field :address %>
  </div>
  <div class="form-group">
    <%= f.label :Ciudad %><br>
    <%= f.text_field :city %>
  </div>
  <div class="form-group">
    <%= f.label :Estado %><br>
    <%= f.text_field :state %>
  </div>
  <div class="form-group">
    <%= f.label :Codigo_Postal %><br>
    <%= f.number_field :zip %>
  </div>
  <div class="form-group">
    <%= f.label :certificado_por %><br>
    <%= f.text_field :certified_by %>
  </div>
  <div class="form-group">
    <%= f.label :experiencia %><br>
    <%= f.number_field :experience %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
  </form>
<% end %>
